{
  "name": "uah-sample-election-dapp",
  "version": "1.0.0",
  "private": true,
  "description": "Sample app for use in pec3",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint ./",
    "dev": "webpack-dev-server",
    "test": "truffle test",
    "build": "webpack",
    "ipfs": "ipfs add ./build/ -r",
    "migrate": "truffle migrate --reset",
    "migrate-rinkeby": "truffle migrate --reset -f 2 --network rinkeby",
    "migrate-ropsten": "truffle migrate --reset -f 2 --network ropsten",
    "publish-rinkeby": "truffle publish --network rinkeby",
    "publish-ropsten": "truffle publish --network ropsten",
    "deploy-rinkeby": "npm run build && npm run migrate-rinkeby",
    "deploy-ropsten": "npm run build && npm run migrate-ropsten",
    "deploy-tomcat": "./deploy-tomcat.sh"
  },
  "author": "Diego B.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dappsar/uah.git"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bip39": "^2.4.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "eth-sig-util": "2.1.0",
    "ethereumjs-testrpc": "^3.0.2",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "5.1.1",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "style-loader": "^0.13.1",
    "truffle": "^4.0.1",
    "truffle-contract": "^1.1.11",
    "truffle-hdwallet-provider": "0.0.6",
    "web3": "^0.20.0",
    "web3-provider-engine": "^13.3.3",
    "webpack": "^2.2.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "openzeppelin-solidity": "^2.0.0"
  }
}
